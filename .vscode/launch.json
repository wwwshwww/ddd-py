{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "_:db_api",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "_tameshi.db_api:app",
                "--reload"
            ],
            "jinja": true
        },
        {
            "name": "_:adapter_ex",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "_tameshi.adapter_ex:app",
                "--reload"
            ],
            "jinja": true
        },
        {
            "name": "_:graphql_with_db",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "_tameshi.ariadne.with_db:app",
                "--reload"
            ],
            "jinja": true
        },
        {
            "name": "_:graphql_signature",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "_tameshi.ariadne.signature:app",
                "--reload"
            ],
            "jinja": true
        },
        {
            "name": "appctx:GraphQL",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "ddd_py.app_ctx.adapter.inbound.graphql.ttt:app",
                "--reload"
            ],
            "jinja": true
        }
    ]
}